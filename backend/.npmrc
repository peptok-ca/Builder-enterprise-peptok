# NPM Configuration for optimized builds
registry=https://registry.npmjs.org/
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
fetch-retries=5
network-timeout=300000
maxsockets=3
audit=false
fund=false
progress=false
# Cache settings
cache-max=86400000
cache-min=10
# Reduce verbosity
loglevel=warn
